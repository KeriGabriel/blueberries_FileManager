<?xml version="1.0" encoding="utf-8"?>
<RunSettings>

	<!-- Parameters used by tests at run time -->
	<TestRunParameters>
		<Parameter name = "bigFile"	value = "BigFile.txt" />
		<Parameter name = "goodFile"  value = "GoodFile.txt" />
		<Parameter name = "goodPath"  value = "Testing/GoodFile.txt" />
		<Parameter name = "goodFileNoExt" value = "GoodFile" />
